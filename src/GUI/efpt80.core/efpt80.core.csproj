<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<AssemblyName>efpt</AssemblyName>
		<RootNamespace>Modelling</RootNamespace>
		<NoWarn>$(NoWarn);EF1001</NoWarn>
	</PropertyGroup>

	<PropertyGroup>
		<PlatformTarget>AnyCPU</PlatformTarget>
	</PropertyGroup>

	<PropertyGroup>
		<DefineConstants>TRACE;DEBUG;CORE60;EFCOMPARE</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
    <Compile Include="..\efpt60.core\*.cs">
      <Link>%(Filename)%(Extension)</Link>
    </Compile>
	</ItemGroup>

   <ItemGroup>
     <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="8.0.0-rc.2.23480.1" />
     <PackageReference Include="EfCore.SchemaCompare" Version="8.0.0-rc2-0002" />
   </ItemGroup>

</Project>
