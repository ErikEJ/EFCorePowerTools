<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<NoWarn>$(NoWarn);EF1001</NoWarn>
		<RootNamespace>RevEng.Core</RootNamespace>
	</PropertyGroup>

	<PropertyGroup>
		<DefineConstants>TRACE;CORE60</DefineConstants>
	</PropertyGroup>

	<ItemGroup>
		<Compile Include="..\RevEng.Core\*.cs" Link="%(Filename)%(Extension)" />
		<Compile Include="..\RevEng.Core\Functions\*.cs" Link="Functions\%(Filename)%(Extension)" />
		<Compile Include="..\RevEng.Core\Routines\*.cs" Link="Routines\%(Filename)%(Extension)" />
		<Compile Include="..\RevEng.Core\Procedures\*.cs" Link="Procedures\%(Filename)%(Extension)" />
	</ItemGroup>

	<ItemGroup>
	  <Compile Remove="..\RevEng.Core\Check.cs" />
	  <Compile Remove="..\RevEng.Core\CommentCSharpEntityTypeGenerator.cs" />
	  <Compile Remove="..\RevEng.Core\CustomTemplateFileService.cs" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="..\RevEng.Core\DbContextExtensions" Link="DbContextExtensions" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Bricelam.EntityFrameworkCore.Pluralizer" Version="1.0.0" />
		<PackageReference Include="Humanizer.Core" Version="2.9.9" />
		<PackageReference Include="Microsoft.Data.SqlClient" Version="2.1.4" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="6.0.0-rtm.21515.16" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="6.0.0-rtm.21515.16" />
		<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer.NetTopologySuite" Version="6.0.0-rtm.21515.16" />
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL.NodaTime" Version="6.0.0-rc.2" />
		<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL.NetTopologySuite" Version="6.0.0-rc.2" />
		<PackageReference Include="Pomelo.EntityFrameworkCore.MySql.NetTopologySuite" Version="6.0.0-preview.7" />
		<PackageReference Include="SimplerSoftware.EntityFrameworkCore.SqlServer.NodaTime" Version="6.0.0-rc.2.21480.0" />
		<PackageReference Include="System.CodeDom" Version="5.0.0" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\ErikEJ.EntityFrameworkCore.6.SqlServer.Dacpac\ErikEJ.EntityFrameworkCore.6.SqlServer.Dacpac.csproj" />
		<ProjectReference Include="..\RevEng.Core.Abstractions\RevEng.Core.Abstractions.csproj" />
		<ProjectReference Include="..\RevEng.Shared\RevEng.Shared.csproj" />
	</ItemGroup>

</Project>
